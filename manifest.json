{
  "manifest_version": 3,
  "name": "API 密钥检测器",
  "version": "1.0",
  "description": "检测 OpenAI、Claude 和 Gemini 的 API 密钥是否可用。",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "API 密钥检测器"
  },
  "permissions": ["storage", "activeTab", "scripting", "sidePanel"],
  "host_permissions": ["<all_urls>"],
  "side_panel": {
    "default_path": "popup.html"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
